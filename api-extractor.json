{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "newlineKind": "lf",
  "mainEntryPointFilePath": "<projectFolder>/dist/src/component/index.d.ts",
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/dist/index.d.ts",
    "omitTrimmingComments": true
  },
  "compiler": {
    "overrideTsconfig": {
      "verbatimModuleSyntax": false
    }
  }
}
